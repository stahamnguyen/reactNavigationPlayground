{
  "rules": {
    "semicolon": true,
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never"
        },
        "singleline": "never"
      }
    ],
    "no-trailing-whitespace": true,
    "interface-over-type-literal": true,
    "no-shadowed-variable": true
  }
}
